@import increment
startCount = 5
dynamic title = "My website"

html:
    head:
        title: {title}
    body:
        (Increment
            p:
                "Look at my increment module! (it starts counting at "
                &startCount
                ")"
            @increment(&startCount)
            p: "but this one starts at 3:"
            @increment(3)
        )
style:
    Increment:
        font-size: "20px"
        background-color: "beige"