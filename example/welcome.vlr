@import increment
startCount = 5

html:
    head:
        title: "Test website"
    body:
        (Increment
            p:
                "Look at my increment module! (it starts counting at "
                &startCount
                ")"
            @increment(&startCount)
        )
style:
    Increment:
        font-size: "20px"
        background-color: "beige"